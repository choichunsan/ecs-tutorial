{
  "family": "task-def-for-ecs-tutorial",
  "executionRoleArn": "arn:aws:iam::414207599053:role/ecsTaskExecution",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "project-sgn-prod-ecr-nginx",
      "image": "588483912375.dkr.ecr.ap-northeast-2.amazonaws.com/project-sgn-prod-ecr-nginx:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}